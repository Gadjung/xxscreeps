{
  "name": "xxscreeps",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "eslint": "eslint $(git ls-files '*.ts' '*.tsx')",
    "install": "tsc -b",
    "start": "xxscreeps core/engine/service/launcher.ts",
    "watch": "tsc -b -w"
  },
  "bin": {
    "xxscreeps": "./run"
  },
  "exports": {
    "./config/mods/import/*": "./dist/core/config/mods.resolved/*",
    "./mods/*": "./dist/mods/*",
    "./*": "./dist/core/*"
  },
  "workspaces": [
    "core/backend",
    "core/driver"
  ],
  "dependencies": {
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "4.14.168",
    "@types/lokijs": "^1.5.3",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@xxscreeps/path-finder": "file:core/driver/path-finder",
    "acorn": "^8.0.5",
    "ajv": "^6.12.6",
    "js-yaml": "^3.14.1",
    "json-schema": "^0.3.0",
    "lodash": "^3.10.1",
    "lokijs": "^1.5.11",
    "minimist": "^1.2.5",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "typescript-json-schema": "^0.49.0",
    "xxscreeps": "."
  }
}
